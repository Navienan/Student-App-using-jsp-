  </servlet>
  <servlet-mapping>
  	<servlet-name>displayBasedOnMarksForm</servlet-name>
  	<url-pattern>/display-based-on-marks-students-form</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>displayBasedOnMarks</servlet-name>
  	<jsp-file>/WEB-INF/jsp_files/display-based-on-marks-students.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>displayBasedOnMarks</servlet-name>
  	<url-pattern>/display-based-on-marks-students</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>forgetPasswordForm</servlet-name>
  	<jsp-file>/WEB-INF/jsp_files/forget-password-form.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>displayBasedOnMarks</servlet-name>
  	<url-pattern>/forget-password-form</url-pattern>
  </servlet-mapping>
  
</web-app>